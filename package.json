{
  "name": "binance-listing-bot",
  "version": "3.0.0",
  "description": "Professional Binance new listing trading bot with WebSocket monitoring",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "setup": "node scripts/setup.js",
    "migrate": "node scripts/migrate.js",
    "docker:build": "docker build -t binance-listing-bot .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:logs": "pm2 logs"
  },
  "keywords": [
    "binance",
    "trading",
    "bot",
    "cryptocurrency",
    "websocket",
    "automated-trading"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "node-binance-api": "^0.13.1",
    "dotenv": "^16.3.1",
    "ws": "^8.14.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "sqlite3": "^5.1.6",
    "sequelize": "^6.35.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "joi": "^17.11.0",
    "node-cron": "^3.0.3",
    "p-queue": "^7.4.1",
    "p-retry": "^5.1.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "axios": "^1.6.2",
    "node-telegram-bot-api": "^0.64.0",
    "nodemailer": "^6.9.7",
    "bull": "^4.11.5",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "supertest": "^6.3.3",
    "@types/node": "^20.10.4"
  },
  "optionalDependencies": {
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "mysql2": "^3.6.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
}